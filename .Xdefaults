URxvt.termName:     rxvt

!Font
URxvt.font:         xft:Terminus:style=Regular:pixelsize=16:antialias=true

URxvt.borderLess: false

! Scrollbar
URxvt.scrollBar:    false

! Real transparency
URxvt.depth:        32

! Tabbedex
URxvt.perl-ext:         tabbedex
URxvt.tabbed.tabbar-fg: 15
URxvt.tabbed.tabbar-bg: 0
